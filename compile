
cd ./src
echo Compiling test_echo_app...
erlc -o ../ebin test_echo.erl test_echo_app.erl test_echo_sup.erl
echo Compiling servers...
erlc -o ../ebin security_server.erl query_process_server.erl ehtml_server.erl
echo Compiling modules...
erlc -o ../ebin data_collection.erl date_manipulation.erl
erlc -o ../ebin test_data.erl
echo Compiling 3rdparty tools...
erlc -o ../ebin mochinum.erl
echo Making Erlang App "test_echo"...
cp test_echo.app.src ../ebin/test_echo.app
echo Done.
cd ..
